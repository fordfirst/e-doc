{
  "name": "cp-love",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ng serve --port 8100 --open",
    "build": "ng build  --aot --output-hashing=all",
    "build-prod": "ng build  --aot --output-hashing=all --prod",
    "test": "ng test",
